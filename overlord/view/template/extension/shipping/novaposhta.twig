												<div class="col-sm-4">
													<input type="text" name="shipping_novaposhta[curl_timeout]" value="{{ novaposhta['curl_timeout'] }}" placeholder="{{ entry_curl_timeout }}" id="input-curl_timeout" class="form-control" />
												</div>
											</div>
										</div>
										<div class="tab-pane fade" id="tab-department">
