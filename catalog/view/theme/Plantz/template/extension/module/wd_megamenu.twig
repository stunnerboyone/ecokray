{% if warning %}
{% else %}
    {% if menu_type == "horizontal" %}
        <div class="main-category-list left-menu">
        <div class="cat-menu">
			<div class="WD-panel-heading">
				{# <span>{{ text_menu_title }}</span> #}
				{# <i class='material-icons'></i> #}
                <!--<i class='material-icons close-icon'>close</i>-->
			</div> 
			<div class="wd-menu horizontal-menu wd-menu-bar" id="wd-menu-{{ module_id }}">
            {% if items %}
                <ul class="ul-top-items">
                    {% for top_item in items %}
                        {% if top_item.status %}
                            <li class="li-top-item{% if top_item.sub_menu_type == "mega" %} mega-menu{% endif %}{% if top_item.has_child %} wd-sub-menu{% endif %}{% if top_item.sub_menu_type == "flyout" %} flyout-menu{% endif %}">
                                {% if top_item.has_link %}
                                    <a class="a-top-link" href="{{ top_item.link }}">
                                        {% if top_item.icon %}
                                            <img src="{{ top_item.icon }}" class="top-icon"/>
                                        {% endif %}
                                        {% if top_item.has_title %}
                                            <span>{{ top_item.title }}</span>
                                        {% endif %}
                                    </a>
                                {% else %}
                                    <a class="a-top-link a-item" href="javascript:void(0)">
                                        {% if top_item.icon %}
                                            <img src="{{ top_item.icon }}" class="top-icon"/>
                                        {% endif %}
                                        {% if top_item.has_title %}
                                            <span>{{ top_item.title }}</span>
                                        {% endif %}
                                    </a>
                                {% endif %}
                                {% if top_item.has_child %}
                                    <!-- Mega Menu -->
                                    {% if top_item.sub_menu_type == "mega" %}
                                        {% if top_item.sub_menu_content_type == 'children' %}
                                        
                                            <div class="mega-menu-container product-thumb sub-menu-container {% if menu_setting.mega_menu_width == '100%' %} full-width {% endif %}">
                                                <div class="container">
                                                {% if top_item.sub_items|length > 0 %}
                                                    {% for sub_item2 in top_item.sub_items %}
                                                        {% if sub_item2.status %}
                                                            <div class="{% if top_item.sub_menu_content_columns %}col-sm-{{ top_item.sub_menu_content_columns }}{% endif %} sub-item2-content"
                                                                    {% if top_item.sub_menu_content_columns == false %} style="width: 20%;" data-cols="12/5" {% else %} data-cols="{{ top_item.sub_menu_content_columns }}" {% endif %} >
                                                                <a class="a-mega-second-link" href="{{ sub_item2.link }}"><h4><strong>{{ sub_item2.title }}</strong></h4></a>
                                                                {% if sub_item2.sub_items|length > 0 %}
                                                                    <div class="sub_item3-content">
                                                                        {% for sub_item3 in sub_item2.sub_items %}
                                                                            {% if sub_item3.status %}
                                                                                <a class="a-mega-third-link" href="{{ sub_item3.link }}"><h5>{{ sub_item3.title }}</h5></a>
                                                                            {% endif %}
                                                                        {% endfor %}
                                                                    </div>
                                                                {% endif %}
                                                            </div>
                                                        {% endif %}
                                                    {% endfor %}
                                                {% endif %}
                                            </div>
                                             </div>
                                        {% endif %}

                                        {% if top_item.sub_menu_content_type == 'category' %}
                                            {% if top_item.sub_menu_content|length > 0 %}
                                                <div class="mega-menu-container product-thumb sub-menu-container {% if menu_setting.mega_menu_width == '100%' %} full-width {% endif %}">
                                                    {% for type,widgets in top_item.sub_menu_content %}
                                                        {% if type == "category" %}
                                                            {% if widgets|length > 0 %}
                                                                {% for widget in widgets %}
                                                                    <div class="col-sm-{{ widget.cols }} sub-item2-content" data-cols="{{ widget.cols }}">
																		<div class="sub-cate-img">
																		{% if widget.image %}
                                                                            <img src="{{ widget.image }}" alt="{{ widget.title }}" class="mega-second-image" />
                                                                        {% endif %}
                                                                        </div>
																		<a class="a-mega-second-link" href="{{ widget.link }}"><h4><strong>{{ widget.title }}</strong></h4></a>
                                                                        {% if widget.children|length > 0 %}
                                                                            <div class="sub_item3-content">
                                                                                {% for c_category in widget.children %}
                                                                                    <a class="a-mega-third-link" href="{{ c_category.link }}"><h5>{{ c_category.title }}</h5></a>
                                                                                {% endfor %}
                                                                            </div>
                                                                        {% endif %}
                                                                       
                                                                    </div>
                                                                {% endfor %}
                                                            {% endif %}
                                                        {% endif %}
                                                    {% endfor %}
                                                </div>
                                            {% endif %}
                                        {% endif %}

                                        {% if top_item.sub_menu_content_type == 'widget' %}
                                            {% if top_item.sub_menu_content|length > 0 %}
                                                <div class="mega-menu-container product-thumb sub-menu-container {% if menu_setting.mega_menu_width == '100%' %} full-width {% endif %}">
                                                    <div class="container">
                                                    {% for type,widgets in top_item.sub_menu_content %}
                                                        {% if type == "widget" %}
                                                            {% if widgets|length > 0 %}
                                                                {% for widget in widgets %}
                                                                    {% if widget.type == "category" %}
                                                                        <div class="col-sm-{{ widget.cols }} sub-item2-content" data-cols="{{ widget.cols }}">
                                                                            <div class="sub-cate-img">
																			{% if widget.image %}
                                                                                <img src="{{ widget.image }}" alt="{{ widget.title }}" class="mega-second-image" />
                                                                            {% endif %}
																			</div>
                                                                            <a class="a-mega-second-link" href="{{ widget.link }}"><h4><strong>{{ widget.title }}</strong></h4></a>
                                                                            {% if widget.children|length > 0 %}
                                                                                <div class="sub_item3-content">
                                                                                    {% for c_category in widget.children %}
                                                                                        <a class="a-mega-third-link" href="{{ c_category.link }}"><h5>{{ c_category.title }}</h5></a>
                                                                                    {% endfor %}
                                                                                </div>
                                                                            {% endif %}
                                                                        </div>
                                                                    {% endif %}

                                                                    {% if widget.type == "product" %}
                                                                        <div class="col-sm-{{ widget.cols }} sub-item2-content div-product" data-cols="{{ widget.cols }}">
                                                                            {% if widget.image %}
                                                                                <div class="product-img">
                                                                                <a class="a-mega-second-link" href="{{ widget.link }}">
                                                                                    <img src="{{ widget.image }}" alt="{{ widget.title }}" class="mega-second-image" />
                                                                                </a>
                                                                                </div>
                                                                            {% endif %}
                                                                            <a class="a-mega-second-link detail" href="{{ widget.link }}">
                                                                                <h4><strong>{{ widget.title }}</strong></h4>
                                                                                {% if widget.price %}
                                                                                    <p class="price">
                                                                                        {% if not widget.special %}
                                                                                            {{ widget.price }}
                                                                                        {% else %}
                                                                                            <span class="price-new">{{ widget.special }}</span> <span class="price-old">{{ widget.price }}</span>
                                                                                        {% endif %}
                                                                                        {% if widget.tax %}
                                                                                            <span class="price-tax">{{ text_tax }} {{ widget.tax }}</span>
                                                                                        {% endif %}
                                                                                    </p>
                                                                                {% endif %} 
                                                                            </a>
                                                                        </div>
                                                                    {% endif %}

                                                                    {% if widget.type == "html" %}
                                                                        <div class="col-sm-{{ widget.cols }} sub-item2-content" data-cols="{{ widget.cols }}">
                                                                            <h4><strong>{{ widget.title }}</strong></h4>
                                                                            {{ widget.content }}
                                                                        </div>
                                                                    {% endif %}

                                                                    {% if widget.type == "link" %}
                                                                        <div class="col-sm-{{ widget.cols }} sub-item2-content" data-cols="{{ widget.cols }}">
                                                                            <a class="a-mega-second-link a-widget-link" href="{{ widget.link }}"><h4><strong>{{ widget.title }}</strong></h4></a>
                                                                        </div>
                                                                    {% endif %}
                                                                {% endfor %}
                                                            {% endif %}
                                                        {% endif %}
                                                    {% endfor %}
                                                </div>
                                                </div>
                                            {% endif %}
                                        {% endif %}
                                    {% endif %}

                                    <!-- Flyout Menu -->
                                    {% if top_item.sub_menu_type == "flyout" %}
                                        {% if top_item.sub_menu_content_type == 'children' %}
                                            {% if top_item.sub_items|length > 0 %}
                                                <div class="wd-sub-menu-container sub-menu-container">
                                                    <ul class="ul-second-items">
                                                        {% for sub_item2 in top_item.sub_items %}
                                                            {% if sub_item2.status %}
                                                                <li class="li-second-items">
                                                                    <a href="{{ sub_item2.link }}" class="a-second-link a-item">
                                                                        <span class="a-second-title">{{ sub_item2.title }}</span>
                                                                        {% if sub_item2.sub_items|length > 0 %}
                                                                                <i class="material-icons chevron-right">chevron_right</i>
                                                                        {% endif %}
                                                                    </a>
                                                                    {% if sub_item2.sub_items|length > 0 %}
                                                                        <div class="flyout-third-items">
                                                                            <ul class="ul-third-items">
                                                                                {% for sub_item3 in sub_item2.sub_items %}
                                                                                    {% if sub_item3.status %}
                                                                                        <li class="li-third-items">
                                                                                            <a href="{{ sub_item3.link }}" class="a-third-link"><span class="a-third-title">{{ sub_item3.title }}</span></a>
                                                                                        </li>
                                                                                    {% endif %}
                                                                                {% endfor %}
                                                                            </ul>
                                                                        </div>
                                                                    {% endif %}
                                                                </li>
                                                            {% endif %}
                                                        {% endfor %}
                                                    </ul>
                                                </div>
                                            {% endif %}
                                        {% endif %}

                                        {% if top_item.sub_menu_content_type == 'category' %}
                                            {% if top_item.sub_menu_content|length > 0 %}
                                                <div class="wd-sub-menu-container sub-menu-container">
                                                    <ul class="ul-second-items">
                                                        {% for type,widgets in top_item.sub_menu_content %}
                                                            {% if type == "category" %}
                                                                {% if widgets|length > 0 %}
                                                                    {% for widget in widgets %}
                                                                        <li class="li-second-items">
                                                                            <a href="{{ widget.link }}" class="a-second-link a-item">
                                                                                <span class="a-second-title">{{ widget.title }}</span>
                                                                                {% if widget.children|length > 0 %}
                                                                                        <i class="material-icons chevron-right">chevron_right</i>
                                                                                {% endif %}
                                                                            </a>
                                                                            {% if widget.children|length > 0 %}
                                                                                <div class="flyout-third-items">
                                                                                    <ul class="ul-third-items">
                                                                                        {% for c_category in widget.children %}
                                                                                            <li class="li-third-items">
                                                                                                <a href="{{ c_category.link }}" class="a-third-link"><span class="a-third-title">{{ c_category.title }}</span></a>
                                                                                            </li>
                                                                                        {% endfor %}
                                                                                    </ul>
                                                                                </div>
                                                                            {% endif %}
                                                                        </li>
                                                                    {% endfor %}
                                                                {% endif %}
                                                            {% endif %}
                                                        {% endfor %}
                                                    </ul>
                                                </div>
                                            {% endif %}
                                        {% endif %}
                                    {% endif %}
                                {% endif %}
                            </li>
                        {% endif %}
						
                    {% endfor %}
					
                </ul>
            {% endif %}
        	</div>
		</div>
		</div>
    {% endif %}

    {% set item_show = menu_setting.item_show %}
    {% if menu_type == "vertical" %}
        {% set count = 0 %}
        <div class="wd-menu vertical-menu" id="wd-menu-{{ module_id }}">
            {% if items %}
                <div class="wd-menu-bar">
                    <div class="left"><i class="material-icons icon-menu">menu</i> <span>{{ text_vertical_bar }}</span></div>
                    <div class="right"><i class="material-icons expand-more">expand_more</i></div>
                </div>
                <ul class="ul-top-items">
                    {% for top_item in items %}
                        {% if top_item.status %}
                            {% set count = count + 1 %}
                            <li class="li-top-item {{ (count > item_show) ? 'over' : '' }}">
                                {% if top_item.has_link %}
                                    <a class="a-top-link a-item" href="{{ top_item.link }}">
                                        {% if top_item.icon %}
                                            <img src="{{ top_item.icon }}" class="top-icon"/>
                                        {% endif %}
                                        {% if top_item.has_title %}
                                            <span>{{ top_item.title }}</span>
                                        {% endif %}
                                        
                                    </a>
                                {% else %}
                                    <a class="a-top-link a-item" href="javascript:void(0)">
                                        {% if top_item.icon %}
                                            <img src="{{ top_item.icon }}" class="top-icon"/>
                                        {% endif %}
                                        {% if top_item.has_title %}
                                            <span>{{ top_item.title }}</span>
                                        {% endif %}
                                    </a>
                                {% endif %}
                                {% if top_item.has_child %}
                                    <!-- Mega Menu -->
                                    {% if top_item.sub_menu_type == "mega" %}
                                        {% if top_item.sub_menu_content_type == 'children' %}
                                       
                                            <div class="mega-menu-container product-thumb sub-menu-container {% if menu_setting.mega_menu_width == '100%' %} full-width {% endif %}">
                                                <div class="container">
                                                {% if top_item.sub_items|length > 0 %}
                                                    {% for sub_item2 in top_item.sub_items %}
                                                        {% if sub_item2.status %}
                                                            <div class="{% if top_item.sub_menu_content_columns %}col-sm-{{ top_item.sub_menu_content_columns }}{% endif %} sub-item2-content"
                                                                    {% if top_item.sub_menu_content_columns == false %} style="width: 20%;" data-cols="12/5" {% else %} data-cols="{{ top_item.sub_menu_content_columns }}" {% endif %} >
                                                                <a class="a-mega-second-link" href="{{ sub_item2.link }}"><h4><strong>{{ sub_item2.title }}</strong></h4></a>
                                                                {% if sub_item2.sub_items|length > 0 %}
                                                                    <div class="sub_item3-content">
                                                                        {% for sub_item3 in sub_item2.sub_items %}
                                                                            {% if sub_item3.status %}
                                                                                <a class="a-mega-third-link" href="{{ sub_item3.link }}"><h5>{{ sub_item3.title }}</h5></a>
                                                                            {% endif %}
                                                                        {% endfor %}
                                                                    </div>
                                                                {% endif %}
                                                            </div>
                                                        {% endif %}
                                                    {% endfor %}
                                                {% endif %}
                                            </div>
                                        {% else %}
                                            {% if top_item.sub_menu_content|length > 0 %}
                                                <div class="mega-menu-container product-thumb sub-menu-container {% if menu_setting.mega_menu_width == '100%' %} full-width {% endif %}">
                                                    {% for type,widgets in top_item.sub_menu_content %}
                                                        {% if type == "category" %}
                                                            {% if widgets|length > 0 %}
                                                                {% for widget in widgets %}
                                                                    <div class="col-sm-{{ widget.cols }} sub-item2-content" data-cols="{{ widget.cols }}">
                                                                        <div class="sub-cate-img">
																		{% if widget.image %}
                                                                            <img src="{{ widget.image }}" alt="{{ widget.title }}" class="mega-second-image" />
                                                                        {% endif %}
																		</div>
																		<a class="a-mega-second-link" href="{{ widget.link }}"><h4><strong>{{ widget.title }}</strong></h4></a>
                                                                        {% if widget.children|length > 0 %}
                                                                            <div class="sub_item3-content">
                                                                                {% for c_category in widget.children %}
                                                                                    <a class="a-mega-third-link" href="{{ c_category.link }}"><h5>{{ c_category.title }}</h5></a>
                                                                                {% endfor %}
                                                                            </div>
                                                                        {% endif %}
                                                                    </div>
                                                                {% endfor %}
                                                            {% endif %}
                                                        {% endif %}

                                                        {% if type == "widget" %}
                                                            {% if widgets|length > 0 %}
                                                                {% for widget in widgets %}
                                                                    {% if widget.type == "category" %}
                                                                        <div class="col-sm-{{ widget.cols }} sub-item2-content" data-cols="{{ widget.cols }}">
                                                                            <div class="sub-cate-img">
																			{% if widget.image %}
                                                                                <img src="{{ widget.image }}" alt="{{ widget.title }}" class="mega-second-image" />
                                                                            {% endif %}
																			</div>
                                                                            <a class="a-mega-second-link" href="{{ widget.link }}"><h4><strong>{{ widget.title }}</strong></h4></a>
                                                                            {% if widget.children|length > 0 %}
                                                                                <div class="sub_item3-content">
                                                                                    {% for c_category in widget.children %}
                                                                                        <a class="a-mega-third-link" href="{{ c_category.link }}"><h5>{{ c_category.title }}</h5></a>
                                                                                    {% endfor %}
                                                                                </div>
                                                                            {% endif %}
                                                                        </div>
                                                                    {% endif %}

                                                                    {% if widget.type == "product" %}
                                                                        <div class="col-sm-{{ widget.cols }} sub-item2-content div-product" data-cols="{{ widget.cols }}">
                                                                            {% if widget.image %}
                                                                                <img src="{{ widget.image }}" alt="{{ widget.title }}" class="mega-second-image" />
                                                                            {% endif %}
                                                                            <a class="a-mega-second-link" href="{{ widget.link }}"><h4><strong>{{ widget.title }}</strong></h4><div class="price">{{ widget.price }}</div></a>
                                                                        </div>
                                                                        
                                                                    {% endif %}

                                                                    {% if widget.type == "html" %}
                                                                        <div class="col-sm-{{ widget.cols }} sub-item2-content" data-cols="{{ widget.cols }}">
                                                                            <h4><strong>{{ widget.title }}</strong></h4>
                                                                            {{ widget.content }} 
                                                                        </div>
                                                                    {% endif %}

                                                                    {% if widget.type == "link" %}
                                                                        <div class="col-sm-{{ widget.cols }} sub-item2-content" data-cols="{{ widget.cols }}">
                                                                            <a class="a-mega-second-link a-widget-link" href="{{ widget.link }}"><h4><strong>{{ widget.title }}</strong></h4></a>
                                                                        </div>
                                                                    {% endif %}
                                                                {% endfor %}
                                                            {% endif %}
                                                        {% endif %}
                                                    {% endfor %}
                                                </div>
                                            {% endif %}
                                            </div>
                                        {% endif %}
                                    {% endif %}

                                    <!-- Flyout Menu -->
                                    {% if top_item.sub_menu_type == "flyout" %}
                                        {% if top_item.sub_menu_content_type == 'children' %}
                                            {% if top_item.sub_items|length > 0 %}
                                                <div class="wd-sub-menu-container sub-menu-container">
                                                    <ul class="ul-second-items">
                                                        {% for sub_item2 in top_item.sub_items %}
                                                            {% if sub_item2.status %}
                                                                <li class="li-second-items">
                                                                    <a href="{{ sub_item2.link }}" class="a-second-link a-item">
                                                                        <span class="a-second-title">{{ sub_item2.title }}</span>
                                                                        {% if sub_item2.sub_items|length > 0 %}
                                                                            <i class="material-icons chevron-right">chevron_right</i>
                                                                        {% endif %}
                                                                    </a>
                                                                    {% if sub_item2.sub_items|length > 0 %}
                                                                        <div class="flyout-third-items">
                                                                            <ul class="ul-third-items">
                                                                                {% for sub_item3 in sub_item2.sub_items %}
                                                                                    {% if sub_item3.status %}
                                                                                        <li class="li-third-items">
                                                                                            <a href="{{ sub_item3.link }}" class="a-third-link"><span class="a-third-title">{{ sub_item3.title }}</span></a>
                                                                                        </li>
                                                                                    {% endif %}
                                                                                {% endfor %}
                                                                            </ul>
                                                                        </div>
                                                                    {% endif %}
                                                                </li>
                                                            {% endif %}
                                                        {% endfor %}
                                                    </ul>
                                                </div>
                                            {% endif %}
                                        {% endif %}

                                        {% if top_item.sub_menu_content_type == 'category' %}
                                            {% if top_item.sub_menu_content|length > 0 %}
                                                <div class="wd-sub-menu-container sub-menu-container">
                                                    <ul class="ul-second-items">
                                                        {% for type,widgets in top_item.sub_menu_content %}
                                                            {% if type == "category" %}
                                                                {% if widgets|length > 0 %}
                                                                    {% for widget in widgets %}
                                                                        <li class="li-second-items">
                                                                            <a href="{{ widget.link }}" class="a-second-link a-item">
                                                                                <span class="a-second-title">{{ widget.title }}</span>
                                                                                {% if widget.children|length > 0 %}
                                                                                    <i class="material-icons chevron-right">chevron_right</i>
                                                                                {% endif %}
                                                                            </a>
                                                                            {% if widget.children|length > 0 %}
                                                                                <div class="flyout-third-items">
                                                                                    <ul class="ul-third-items">
                                                                                        {% for c_category in widget.children %}
                                                                                            <li class="li-third-items">
                                                                                                <a href="{{ c_category.link }}" class="a-third-link"><span class="a-third-title">{{ c_category.title }}</span></a>
                                                                                            </li>
                                                                                        {% endfor %}
                                                                                    </ul>
                                                                                </div>
                                                                            {% endif %}
                                                                        </li>
                                                                    {% endfor %}
                                                                {% endif %}
                                                            {% endif %}
                                                        {% endfor %}
                                                    </ul>
                                                </div>
                                            {% endif %}
                                        {% endif %}
                                    {% endif %}
                                {% endif %}
                            </li>
                        {% endif %}
                    {% endfor %}
				
                    {% if count > item_show %}
                        <li class="li-top-item li-over li-plus">
                            <a href="javascript:void(0);" class="a-top-link a-plus a-over"><i class="material-icons add-icon" aria-hidden="true">add</i> <span>More Items</span></a>
                        </li>
                        <li class="li-top-item li-over li-minus over">
                            <a href="javascript:void(0);" class="a-top-link a-minus a-over"><i class="material-icons remove-icon">remove</i> <span>Close Items</span></a>
                        </li>
                    {% endif %}
					
                </ul>
            {% endif %}
        </div>
    {% endif %}

    
{% endif %}